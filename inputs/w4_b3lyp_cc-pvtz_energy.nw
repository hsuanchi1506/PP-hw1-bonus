echo

start w4_b3lyp_cc-pvtz_energy

memory stack 8000 mb heap 100 mb global 8000 mb noverify

permanent_dir .
scratch_dir /tmp

geometry units angstrom
  O      -1.367062       1.364510       0.007273
  O      -1.364510      -1.367062      -0.007273
  O       1.364510       1.367062      -0.007273
  O       1.367062      -1.364510       0.007273
  H      -0.395152       1.503429      -0.005375
  H      -1.503429      -0.395152       0.005375
  H       1.503429       0.395152       0.005375
  H       0.395152      -1.503429      -0.005375
  H      -1.687281       1.875361       0.755434
  H      -1.875361      -1.687281      -0.755434
  H       1.875361       1.687281      -0.755434
  H       1.687281      -1.875361       0.755434
end

basis "ao basis" spherical noprint
  * library cc-pvtz
end

scf
  direct
  singlet
  rhf
  thresh 1e-7
  maxiter 100
  vectors input atomic output w4_scf_cc-pvtz.movecs
  noprint "final vectors analysis" "final vector symmetries"
end

task scf energy ignore

dft
  direct
  xc b3lyp
  grid fine
  iterations 100
  vectors input w4_scf_cc-pvtz.movecs
  noprint "final vectors analysis" "final vector symmetries"
end

task dft energy

